          "accInput": {
            "type": "string",
            "description": "Tên đăng nhập của người dùng"
          },
          "password": {
            "type": "string",
            "description": "Mật khẩu của người dùng"
          }
        },
        "required": [
          "accInput",
          "password"
        ]
      },
      "ResponseAuthDto": {
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string",
            "description": "Access token trả về sau khi đăng nhập hoặc refresh"
          },
          "refreshToken": {
            "type": "string",
            "description": "Refresh token đi kèm với access token"
          }
        }
      },
      "RegisterDto": {
        "type": "object",
        "properties": {
          "password": {
            "type": "string",
            "description": "Mật khẩu mạnh (tối thiểu 8 ký tự, 1 số, 1 chữ in hoa, 1 ký tự đặc biệt)",
            "example": "StrongP@ssw0rd"
          },
          "accInput": {
            "type": "string",
            "description": "Phone number or email",
            "example": "0123456789 or abc@gmail.com"
          }
        },
        "required": [
          "password",
          "accInput"
        ]
      },
